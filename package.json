{
  "name": "bo-test",
  "version": "1.0.0",
  "description": "BO test",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbestmaster/bo-test.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webbestmaster/bo-test/issues"
  },
  "homepage": "https://github.com/webbestmaster/bo-test#readme",
  "scripts": {
    "test": "mocha --require @babel/register ./src/test/*",
    "check": "npm run eslint; npm run flow;",
    "check:fix": "npm run prettier; npm run eslint:fix; npm run flow;",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "flow": "flow stop; flow;",
    "prettier": "prettier --write \"./src/**/*.js\"",
    "mdlint": "eslint --ext md .",
    "mdlint:fix": "eslint --ext md . --fix",
    "flow-remove-types": "flow-remove-types --pretty ./src/ --out-dir dist/",
    "flow-coverage": "flow-coverage-report --config ./flow-coverage-config.json"
  },
  "dependencies": {
    "mocha": "^6.0.2",
    "puppeteer": "^1.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/plugin-transform-strict-mode": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-flow-runtime": "^0.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "base64-inline-loader": "^1.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-flowtype": "^3.5.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.4.0",
    "eslint-plugin-optimize-regex": "^1.1.6",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-sonarjs": "^0.3.0",
    "eslint-plugin-unicorn": "^8.0.1",
    "flow-bin": "^0.96.0",
    "flow-coverage-report": "^0.6.1",
    "flow-remove-types": "^1.2.3",
    "flow-runtime": "^0.17.0",
    "flow-typed": "^2.5.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4"
  }
}
